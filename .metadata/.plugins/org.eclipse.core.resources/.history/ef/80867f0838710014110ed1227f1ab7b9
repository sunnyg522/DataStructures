package com.vikram;

public class TestScore {
	double average =0;
	public TestScore(double number[])
	{
		try{
		for(int i=0;i<number.length;i++)
		{
			if(number[i]<0 || number[i]>100)
				throw new Exception("Error: Invalid score: "+number[i]);
			else 
				average = average+number[i];
		}
		}
		catch(Exception e)
		{
			e.printStackTrace();
		}
	}

}

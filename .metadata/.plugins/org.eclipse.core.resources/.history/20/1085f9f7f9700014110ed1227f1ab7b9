package com.vikram;

public class TestCustomeNUllException {

	public static void main(String[] args) {
		String array;
		printArrary(array);

	}
	public static void printArrary(String array[])
	{
		if(array[] == null)
			throw new CutomeNUllpointer();
		for(int i = 0;i < array[].length;i++)
		{
			System.out.println(array[][i]);
		}
	}

}
